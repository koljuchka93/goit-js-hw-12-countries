{{#each this}}
<h2 class="country__Name">{{name}}</h2>
<div class="country__Content">
    <div class="country__Box">
        <p class="country__Text">
            Capital: {{capital}}
        </p>
        <p class="country__Text">
            Population: {{population}}
        </p>
        <p class="country__Text">
            Languages:
        <ul class="country__Languages-list">
            {{#each languages}}
            <li>{{this.name}}</li>
            {{/each}}
        </ul>
        </p>
    </div>
    <img src="{{flag}}" alt="{{demonym}}" class="country__img">
</div>
{{/each}}